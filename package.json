{
	"name": "json-diff-git",
	"version": "0.0.1",
	"description": "JSON Diff Git",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"dev": "tsx --watch src/index.ts",
		"build": "rm -rf dist && tsup src/index.ts --format esm --dts"
	},
	"keywords": [
		"diff",
		"json",
