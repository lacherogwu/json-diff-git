		"json-git-diff",
		"json-diff-git",
		"object-diff"
	],
	"author": "LacheRo",
	"license": "MIT",
	"type": "module",
	"devDependencies": {
		"tsup": "^8.0.2",
		"tsx": "^4.7.2",
		"typescript": "^5.4.5"
	},
	"dependencies": {
		"json-diff-kit": "^1.0.23",
		"shiki": "^1.3.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/lacherogwu/json-git-diff"
